named.container {
    background-color: #f7f4ee;
    color: #424242;
}

.inner {
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    padding: 0 50rpx;
}

form {
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.named input {
    text-align: center;
    height: 78rpx;
    border-radius: 10rpx;
    border: 3rpx solid #d9ba6a;
    background-color: #fff;
    line-height: 70rpx;
    font-size: 32rpx;
}

.more input {
    border: 2rpx solid #e7404d;
    box-shadow: 0 0 5px #e7404d;
}

.hd {
    display: flex;
    justify-content: center;
    font-size: 30rpx;
    padding-top: 60rpx;
}

.submit-btn {
    width: 100%;
    height: 94rpx;
    line-height: 94rpx;
    background-color: #d9ba6a;
    border-radius: 10rpx;
    font-size: 36rpx;
    color: #fff;
    font-weight: bold;
    margin-top: 100rpx;
    text-align: center;
}

.submit-btn::after {
    display: none;
}

.labeled {
    width: 43%;
    display: flex;
    border-radius: 5%;
    margin-top: 10px;
    margin-left: 10px;
    border: 2px solid #f1f1f1;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 12px;
    box-sizing: border-box;
    padding-bottom: 10px;
    padding-top: 10px;
}

.labeledfu {
    display: flex;
    flex-wrap: wrap;
    width: 100%;
}

.select {
    border-radius: 5%;
    border: 2px solid #d3483e;
    opacity: 1;
}

.scview {
    height: 300px;
    margin-top: 10px;
    margin-left: 5px;
    margin-right: 5px;
}

.itemtext {
    /* font-weight: bolder; */
    color: #4d4d4d;
}
